{
 "comet": {
    "name": "Comet",
    "type_": "public server",
    "base_url": "https://magnify.pages.dev/scraper/comet",
    "response_type": "json",
    "movie": {
      "query": "/stream/movie/{query}.json",
      "keywords": "{imdbId}"
    },
    "episode": {
      "query": "/stream/series/{query}:{season}:{episode}.json",
      "keywords": "{showImdbId}"
    },
    "anime": {
      "query": "/stream/anime/{query}:{season}:{episode}.json",
      "keywords": "{title}"
    },
    "json_format": {
      "results": "streams",
      "title": "description",
      "size": "behaviorHints.videoSize",
      "hash": "infoHash"
    }
  },
  "zilean-dmm": {
    "name": "DMM",
    "type_": "public server",
    "base_url": "https://magnify.pages.dev/scraper/dmm",
    "response_type": "json",
    "movie": {
      "query": "/?imdbid={query}",
      "keywords": "{imdbId}"
    },
    "episode": {
      "query": "/?imdbid={query}&season={season}&episode={episode}",
      "keywords": "{showImdbId}"
    },
    "json_format": {
      "title": "raw_title",
      "size": "size",
      "hash": "info_hash"
    }
  }
}
