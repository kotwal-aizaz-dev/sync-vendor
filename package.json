{
  "torrents-csv": {
    "name": "TorrentsCSV",
    "type_": "public server",
    "base_url": "https://magnify.pages.dev/scraper/torrents-csv",
    "response_type": "json",
    "movie": {
      "query": "/search?q={query}",
      "keywords": "{title} {year}"
    },
    "episode": {
      "query": "/search?q={query}",
      "keywords": "{title} {episodeCode}"
    },
    "season": {
      "query": "/search?q={query}",
      "keywords": [
        "{title} {seasonCode}",
        "{title} {season}"
      ]
    },
    "anime": {
      "query": "/search?q={query}",
      "keywords": [
        "{title} {absoluteNumber}",
        "{title} {episode}"
      ]
    },
    "json_format": {
      "results": "torrents",
      "title": "name",
      "size": "size_bytes",
      "seeds": "seeders",
      "peers": "leechers",
      "hash": "infohash"
    }
  },
  "animetosho_primary": {
    "name": "AnimeTosho",
    "type_": "public server",
    "base_url": "https://magnify.pages.dev/scraper/animetosho",
    "response_type": "text",
    "movie": {
      "query": "/?q={query}&order=size-d&page=1",
      "keywords": "{title} {year}"
    },
    "episode": {
      "query": "/?q={query}&order=size-d&page=1",
      "keywords": "{title} {episodeCode}"
    },
    "season": {
      "query": "/?q={query}&order=size-d&page=1",
      "keywords": [
        "{title} {seasonCode}",
        "{title} {season}"
      ]
    },
    "anime": {
      "query": "/?q={query}&order=size-d&page=1",
      "keywords": [
        "{title} {absoluteNumber}",
        "{title} {episode}"
      ]
    },
    "html_parser": {
      "row": "doc.querySelectorAll('.home_list_entry')",
      "title": "row.querySelector('.link a').textContent",
      "size": "row.querySelector('.size').textContent",
      "seeds": "row.querySelector('.links').textContent.match(/\\d+/g)[0]",
      "peers": "row.querySelector('.links').textContent.match(/\\d+/g)[1]",
      "url": "row.querySelector('.links a:nth-child(4)').getAttribute('href')"
    }
  }
}
