{
  "zilean-dmm": {
    "name": "DMM",
    "type_": "public server",
    "base_url": "https://magnify.pages.dev/scraper/dmm",
    "response_type": "json",
    "movie": {
      "query": "/?imdbid={query}",
      "keywords": "{imdbId}"
    },
    "episode": {
      "query": "/?imdbid={query}&season={season}&episode={episode}",
      "keywords": "{showImdbId}"
    },
    "json_format": {
      "title": "raw_title",
      "size": "size",
      "hash": "info_hash"
    }
  },
 "torrents-csv": {
    "name": "TorrentsCSV",
    "type_": "public server",
    "base_url": "https://magnify.pages.dev/scraper/torrents-csv",
    "response_type": "json",
    "movie": {
      "query": "/search?q={query}",
      "keywords": "{title} {year}"
    },
    "episode": {
      "query": "/search?q={query}",
      "keywords": "{title} {episodeCode}"
    },
    "season": {
      "query": "/search?q={query}",
      "keywords": [
        "{title} {seasonCode}",
        "{title} {season}"
      ]
    },
    "anime": {
      "query": "/search?q={query}",
      "keywords": [
        "{title} {absoluteNumber}",
        "{title} {episode}"
      ]
    },
    "json_format": {
      "results": "torrents",
      "title": "name",
      "size": "size_bytes",
      "seeds": "seeders",
      "peers": "leechers",
      "hash": "infohash"
    }
  }
}
